package com.flashmastery.as3.microsite.patterns.model {	import com.flashmastery.as3.microsite.patterns.interfaces.IHistoryProxy;	/**	 * @author Stefan von der Krone (2010)	 */	public class FMHistoryProxy extends FMMicrositeProxy implements IHistoryProxy {		public static const NAME : String = "FMHistoryProxy";				protected static const _index : uint = 0;				protected var _historyList : Array;				protected var _current : String;		protected var _last : String;		public function FMHistoryProxy() {			super( getInstanceName(), null );		}		protected function getInstanceName() : String {
			return NAME + _index++;
		}
		public function getFullHistory() : Array {			return _historyList;		}				public function setNext(next : String) : void {			_last = _current;			_historyList.push( _current );			_current = next;		}				public function get current() : String {			return _current;		}				public function get last() : String {			return _last;		}	}}