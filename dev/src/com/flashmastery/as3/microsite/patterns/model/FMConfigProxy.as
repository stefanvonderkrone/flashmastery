package com.flashmastery.as3.microsite.patterns.model {	import com.flashmastery.as3.microsite.interfaces.IStartupConfiguration;	import com.flashmastery.as3.microsite.patterns.interfaces.IConfigProxy;	import com.flashmastery.as3.microsite.patterns.model.vo.FMFlashVarConstantsVO;	import com.flashmastery.as3.microsite.patterns.model.vo.FMMicrositeConfigVO;	/**	 * @author Stefan von der Krone (2010)	 */	public class FMConfigProxy extends FMMicrositeProxy implements IConfigProxy {		public static const NAME : String = "FMConfigProxy";		public function FMConfigProxy(proxyName : String, startupConfig : IStartupConfiguration ) {			super( proxyName, new FMMicrositeConfigVO( startupConfig ) );		}		override public function onRegister() : void {			super.onRegister( );
			parseXML( );
		}
		protected function parseXML() : void {
		}				protected function get configVO () : FMMicrositeConfigVO {			return data as FMMicrositeConfigVO;
		}				public function getDataProviderByMediatorName( mediatorName : String ) : Object {			return configVO.mediatorDataProvider[ mediatorName ];		}				public function setDataProviderByMediatorName( mediatorName : String, data : Object ) : void {			configVO.mediatorDataProvider[ mediatorName ] = data;		}		public function getFlashVarByID( id : String ) : String {			return configVO.flashVars[ id ];		}				public function addStartPageConstant( constant : String, section : String ) : void {			configVO.startPageConstants[ constant ] = section;		}				public function removeStartPageConstant( constant : String ) : void {			delete configVO.startPageConstants[ constant ];		}				public function getStartPage(startPageID : String) : String {			return configVO.flashVars[ startPageID ] as String;		}				public function getStartPageConstant() : String {			if ( configVO.startPageConstants[ startPage ] ) return configVO.startPageConstants[ startPage ];			return configVO.defaultStartPage;		}				public function setDefaultStartPage( section : String ) : void {			configVO.defaultStartPage = section;		}				public function get startPage() : String {			return configVO.flashVars[ FMFlashVarConstantsVO.START_PAGE ] as String;		}	}}