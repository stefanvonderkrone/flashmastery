package com.flashmastery.as3.microsite.patterns.controller.startup {	import com.flashmastery.as3.microsite.interfaces.IStartupConfiguration;	import com.flashmastery.as3.microsite.patterns.controller.FMSimpleCommand;	import org.puremvc.as3.multicore.interfaces.INotification;	/**	 * @author Stefan von der Krone (2010)	 */	public class FMInitializeModelCommand extends FMSimpleCommand {				override public function execute(note : INotification) : void {			var startupConfig : IStartupConfiguration = note.getBody() as IStartupConfiguration;			var configProxyClass : Class = micrositeFacade.configProxyClass;			micrositeFacade.configProxy = new configProxyClass( startupConfig );		}	}}